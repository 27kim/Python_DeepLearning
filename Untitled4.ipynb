   "source": [
    "import random\n",
    "rnd = random.randint(1,100) # 1~100 사이 난수\n",
    "\n",
    "print(rnd)\n",
    "\n",
    "\n",
    "while True :    \n",
    "    a= input(\"1~100 숫자를 입력하세요\")\n",
    "    if rnd == int(a) :\n",
    "        print(\"정답\") \n",
    "        break\n",
    "    elif rnd < int(a) :\n",
    "        print(\"더 크지롱\")            \n",
    "    else :    \n",
    "        print(\"더 작지롱\")           "
   ]
